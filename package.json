{
  "name": "react-vite-ssr",
  "version": "1.0.0",
  "main": "./src/index.ts",
  "type": "module",
  "author": "Lucas Farias",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucasfarias2/react-vite-ssr.git"
  },
  "scripts": {
    "start": "NODE_ENV=production node --experimental-specifier-resolution=node ./dist/index.js",
    "dev": "nodemon ./src/index.ts",
    "build": "npm run build:react && npm run build:server",
    "build:server": "tsc -p tsconfig.server.json",
    "build:react": "npm run build:react:client && npm run build:react:ssr",
    "build:react:ssr": "vite build --outDir ./dist/react/ssr --ssr ./src/server/entries/home.tsx",
    "build:react:client": "vite build --outDir ./dist/react",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --quiet",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "vite": "vite"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.1",
    "@tanstack/react-query": "^4.22.0",
    "axios": "^1.2.3",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.6.2"
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@vitejs/plugin-react": "^3.0.1",
    "@vitest/coverage-c8": "^0.27.1",
    "autoprefixer": "^10.4.13",
    "compression": "^1.7.4",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.7.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "happy-dom": "^8.1.3",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.21",
    "prettier": "^2.8.2",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "serve-static": "^1.15.0",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-tsconfig-paths": "^4.0.3",
    "vitest": "^0.27.1",
    "vitest-fetch-mock": "^0.2.1"
  }
}
